{
  "name": "phil.dev-2020",
  "version": "0.1.0",
  "description": "The website for Phil Lennon (frontendphil)",
  "main": "index.js",
  "private": true,
  "scripts": {
    "clean": "del _site",
    "dev:css": "postcss src/css/site.css -o _site/css/site.css --watch --verbose",
    "dev:eleventy": "eleventy --serve --watch",
    "dev:js": "webpack --config webpack.config.js --watch",
    "build:eleventy": "ELEVENTY_ENV=production eleventy",
    "build:css": "NODE_ENV=production postcss src/css/site.css -o _site/css/site.css",
    "build:js": "webpack -p --config webpack.config.js",
    "analyze:build": "webpack --profile --json > _analyze.json",
    "analyze:serve": "webpack-bundle-analyzer _analyze.json",
    "analyze": "npm-run-all clean analyze:* --print-label",
    "develop": "npm-run-all clean --parallel dev:* --print-label",
    "build": "npm-run-all clean --parallel build:* --print-label"
  },
  "repository": {
    "type": "git",
    "url": "git+https://fephil@github.com/fephil/phil.dev-2020.git"
  },
  "keywords": [
    "portfolio",
    "sanity",
    "eleventy",
    "jamstack",
    "webpack",
    "tailwind"
  ],
  "author": "Phil Lennon <enquiry@phil.dev>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fephil/phil.dev-2020/issues"
  },
  "homepage": "https://phil.dev",
  "devDependencies": {
    "@11ty/eleventy": "^0.11.0",
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@sanity/block-content-to-html": "^1.3.8",
    "@sanity/client": "^1.150.7",
    "autoprefixer": "^9.8.6",
    "babel-loader": "^8.1.0",
    "cssnano": "^4.1.10",
    "del-cli": "^3.0.1",
    "dotenv": "^8.2.0",
    "groq": "^1.150.1",
    "html-minifier": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.2",
    "postcss-import": "^12.0.1",
    "tailwindcss": "^1.8.10",
    "webpack": "^4.44.2",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {
    "date-fns": "^2.16.1"
  }
}
