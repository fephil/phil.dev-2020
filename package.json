{
  "name": "phil.dev-2020",
  "version": "2.0.0",
  "description": "The website for Phil Lennon (frontendphil)",
  "main": "index.js",
  "private": true,
  "scripts": {
    "clean": "del-cli _site",
    "dev:css": "postcss src/css/site.css -o _site/css/site.css --watch --verbose",
    "dev:eleventy": "eleventy --serve --watch",
    "dev:js": "webpack --config webpack.config.js --watch",
    "build:eleventy": "cross-env ELEVENTY_ENV=production eleventy",
    "build:css": "cross-env NODE_ENV=production postcss src/css/site.css -o _site/css/site.css",
    "build:js": "cross-env NODE_ENV=production webpack --config webpack.config.js",
    "analyze:build": "webpack --profile --json > _analyze.json",
    "analyze:serve": "webpack-bundle-analyzer _analyze.json",
    "analyze": "npm-run-all clean analyze:* --print-label",
    "develop": "npm-run-all clean --parallel dev:* --print-label",
    "build": "npm-run-all clean --parallel build:* --print-label",
    "lint": "eslint --fix src/js/**"
  },
  "repository": {
    "type": "git",
    "url": "git+https://fephil@github.com/fephil/phil.dev-2020.git"
  },
  "keywords": [
    "portfolio",
    "sanity",
    "eleventy",
    "jamstack",
    "webpack",
    "tailwind",
    "babel",
    "eslint",
    "prettier",
    "postcss"
  ],
  "author": "Phil Lennon <enquiry@phil.dev>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fephil/phil.dev-2020/issues"
  },
  "homepage": "https://phil.dev",
  "devDependencies": {
    "@11ty/eleventy": "^0.11.1",
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "autoprefixer": "^10.2.1",
    "babel-loader": "^8.2.2",
    "cross-env": "^7.0.3",
    "cssnano": "^4.1.10",
    "del-cli": "^3.0.1",
    "eslint": "^7.18.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-webpack-plugin": "^2.4.1",
    "html-minifier": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.4",
    "postcss-cli": "^8.3.1",
    "postcss-import": "^14.0.0",
    "postcss-nested": "^5.0.3",
    "prettier": "^2.2.1",
    "tailwindcss": "^2.0.2",
    "webpack": "^5.15.0",
    "webpack-bundle-analyzer": "^4.3.0",
    "webpack-cli": "^4.3.1"
  },
  "dependencies": {
    "@sanity/block-content-to-html": "^1.3.8",
    "@sanity/client": "^2.2.6",
    "@sanity/image-url": "^0.140.22",
    "@sentry/browser": "^6.2.0",
    "@sentry/tracing": "^6.2.0",
    "animate.css": "^4.1.1",
    "date-fns": "^2.17.0",
    "dotenv": "^8.2.0",
    "groq": "^2.2.6"
  }
}
